          exit 1
        fi
        ;;

      # :flag.case
      --format | -f)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--format']="$2"
          shift
          shift
        else
          printf "%s\n" "--format requires an argument: --format, -f FORMAT" >&2
          exit 1
        fi
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

  # :command.default_assignments
  [[ -n ${args['--traffic_type']:-} ]] || args['--traffic_type']="application"
  [[ -n ${args['--metric_type']:-} ]] || args['--metric_type']="sumbytes"
  [[ -n ${args['--count']:-} ]] || args['--count']="5"
  [[ -n ${args['--time_period']:-} ]] || args['--time_period']="3m"
  [[ -n ${args['--baseurl']:-} ]] || args['--baseurl']="http://192.168.1.1"
  [[ -n ${args['--format']:-} ]] || args['--format']="yaml"

  # :command.validations
  # :flag.validations
  if [[ -v args['--baseurl'] ]]; then
    validation_output="$(validate_base_url "${args['--baseurl']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--baseurl BASEURL" "$validation_output" >&2
      exit 1
    fi
  fi

  # :flag.validations
  if [[ -v args['--output'] ]]; then
    validation_output="$(validate_file_path "${args['--output']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--output, -o OUTPUT" "$validation_output" >&2
      exit 1
    fi
  fi

  # :command.whitelist_filter
  if [[ ${args['--traffic_type']:-} ]] && [[ ! ${args['--traffic_type']:-} =~ ^(application|group)$ ]]; then
    printf "%s\n" "--traffic_type must be one of: application, group" >&2
    exit 1
  fi
  if [[ ${args['--metric_type']:-} ]] && [[ ! ${args['--metric_type']:-} =~ ^(sumbytes|sumspeed)$ ]]; then
    printf "%s\n" "--metric_type must be one of: sumbytes, sumspeed" >&2
    exit 1
  fi
  if [[ ${args['--time_period']:-} ]] && [[ ! ${args['--time_period']:-} =~ ^(3m|1h|3h|24h)$ ]]; then
    printf "%s\n" "--time_period must be one of: 3m, 1h, 3h, 24h" >&2
    exit 1
  fi
  if [[ ${args['--format']:-} ]] && [[ ! ${args['--format']:-} =~ ^(json|yaml|xml|props)$ ]]; then
    printf "%s\n" "--format must be one of: json, yaml, xml, props" >&2
    exit 1
  fi

}

# :command.parse_requirements
keencli_config_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        keencli_config_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="config"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --baseurl)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--baseurl']="$2"
          shift
          shift
        else
          printf "%s\n" "--baseurl requires an argument: --baseurl BASEURL" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --output | -o)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--output']="$2"
          shift
          shift
        else
          printf "%s\n" "--output requires an argument: --output, -o OUTPUT" >&2
          exit 1
        fi
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        # :argument.case
        if [[ -z ${args['configuration_file']+x} ]]; then
          args['configuration_file']=$1
          shift
        else
          printf "invalid argument: %s\n" "$key" >&2
          exit 1
        fi

        ;;

    esac
  done

  # :command.required_args_filter
  if [[ -z ${args['configuration_file']+x} ]]; then
    printf "missing required argument: CONFIGURATION_FILE\nusage: keencli config CONFIGURATION_FILE [OPTIONS]\n" >&2

    exit 1
  fi

  # :command.default_assignments
  [[ -n ${args['--baseurl']:-} ]] || args['--baseurl']="http://192.168.1.1"

  # :command.validations
  # :flag.validations
  if [[ -v args['--baseurl'] ]]; then
    validation_output="$(validate_base_url "${args['--baseurl']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--baseurl BASEURL" "$validation_output" >&2
      exit 1
    fi
  fi

  # :flag.validations
  if [[ -v args['--output'] ]]; then
    validation_output="$(validate_file_path "${args['--output']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--output, -o OUTPUT" "$validation_output" >&2
      exit 1
    fi
  fi

  # :command.whitelist_filter
  if [[ -n ${args['configuration_file']:-} ]] && [[ ! ${args['configuration_file']:-} =~ ^(startup-config|default-config|running-config)$ ]]; then
    printf "%s\n" "configuration_file must be one of: startup-config, default-config, running-config" >&2
    exit 1
  fi

}

# :command.parse_requirements
keencli_log_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        keencli_log_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="log"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --baseurl)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--baseurl']="$2"
          shift
          shift
        else
          printf "%s\n" "--baseurl requires an argument: --baseurl BASEURL" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --output | -o)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--output']="$2"
          shift
          shift
        else
          printf "%s\n" "--output requires an argument: --output, -o OUTPUT" >&2
          exit 1
        fi
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

  # :command.default_assignments
  [[ -n ${args['--baseurl']:-} ]] || args['--baseurl']="http://192.168.1.1"

  # :command.validations
  # :flag.validations
  if [[ -v args['--baseurl'] ]]; then
    validation_output="$(validate_base_url "${args['--baseurl']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--baseurl BASEURL" "$validation_output" >&2
      exit 1
    fi
  fi

  # :flag.validations
  if [[ -v args['--output'] ]]; then
    validation_output="$(validate_file_path "${args['--output']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--output, -o OUTPUT" "$validation_output" >&2
      exit 1
    fi
  fi

}

# :command.parse_requirements
keencli_self_test_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        keencli_self_test_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="self-test"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --baseurl)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--baseurl']="$2"
          shift
          shift
        else
          printf "%s\n" "--baseurl requires an argument: --baseurl BASEURL" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --output | -o)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--output']="$2"
          shift
          shift
        else
          printf "%s\n" "--output requires an argument: --output, -o OUTPUT" >&2
          exit 1
        fi
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

  # :command.default_assignments
