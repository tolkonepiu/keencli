    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--baseurl BASEURL" "$validation_output" >&2
      exit 1
    fi
  fi

  # :flag.validations
  if [[ -v args['--output'] ]]; then
    validation_output="$(validate_file_path "${args['--output']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--output, -o OUTPUT" "$validation_output" >&2
      exit 1
    fi
  fi

}

# :command.initialize
initialize() {
  declare -g version="0.4.4"
  set -e

}

# :command.run
run() {
  # :command.globals
  declare -g long_usage=''
  declare -g -A args=()
  declare -g -A deps=()
  declare -g -a env_var_names=()
  declare -g -a input=()

  normalize_input "$@"
  parse_requirements "${input[@]}"

  case "$action" in
    "request") keencli_request_command ;;
    "show") keencli_show_command ;;
    "show system") keencli_show_system_command ;;
    "show interface") keencli_show_interface_command ;;
    "show ip") keencli_show_ip_command ;;
    "show ip hotspot") keencli_show_ip_hotspot_command ;;
    "show ip hotspot list") keencli_show_ip_hotspot_list_command ;;
    "show ip hotspot summary") keencli_show_ip_hotspot_summary_command ;;
    "show internet") keencli_show_internet_command ;;
    "show ntce") keencli_show_ntce_command ;;
    "show ntce summary") keencli_show_ntce_summary_command ;;
    "config") keencli_config_command ;;
    "log") keencli_log_command ;;
    "self-test") keencli_self_test_command ;;
  esac
}

if [[ "${BASH_SOURCE[0]}" == "${0}" ]]; then
  initialize
  run "$@"
fi
