keencli_config_usage() {
  printf "keencli config - Configuration files\n\n"

  printf "%s\n" "Usage:"
  printf "  keencli config CONFIGURATION_FILE [OPTIONS]\n"
  printf "  keencli config --help | -h\n"
  echo

  # :command.long_usage
  if [[ -n "$long_usage" ]]; then
    printf "%s\n" "Options:"

    # :command.usage_flags
    # :flag.usage
    printf "  %s\n" "--baseurl BASEURL"
    printf "    Keenetic base URL (example \`http://192.168.1.1\`,\n    \`https://myrouter01.keenetic.link\`)\n"
    printf "    %s\n" "Default: http://192.168.1.1"
    echo

    # :flag.usage
    printf "  %s\n" "--output, -o OUTPUT"
    printf "    Write to file instead of stdout\n"
    echo

    # :command.usage_fixed_flags
    printf "  %s\n" "--help, -h"
    printf "    Show this help\n"
    echo

    # :command.usage_args
    printf "%s\n" "Arguments:"

    # :argument.usage
    printf "  %s\n" "CONFIGURATION_FILE"
    printf "    System configuration files\n    Available options:\n    default-config - factory default settings for this device\n    running-config - current settings for this device\n    startup-config - set of commands that runs when the system starts\n"
    printf "    %s\n" "Allowed: startup-config, default-config, running-config"
    echo

    # :command.usage_examples
    printf "%s\n" "Examples:"
    printf "  keencli config startup-config\n"
    printf "  keencli config default-config --output default-config.txt\n"
    echo

  fi
}

