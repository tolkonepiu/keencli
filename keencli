    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        keencli_show_ip_hotspot_list_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="show ip hotspot list"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --mac_address)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--mac_address']="$2"
          shift
          shift
        else
          printf "%s\n" "--mac_address requires an argument: --mac_address MAC_ADDRESS" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --baseurl)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--baseurl']="$2"
          shift
          shift
        else
          printf "%s\n" "--baseurl requires an argument: --baseurl BASEURL" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --output | -o)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--output']="$2"
          shift
          shift
        else
          printf "%s\n" "--output requires an argument: --output, -o OUTPUT" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --format | -f)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--format']="$2"
          shift
          shift
        else
          printf "%s\n" "--format requires an argument: --format, -f FORMAT" >&2
          exit 1
        fi
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

  # :command.default_assignments
  [[ -n ${args['--baseurl']:-} ]] || args['--baseurl']="http://192.168.1.1"
  [[ -n ${args['--format']:-} ]] || args['--format']="yaml"

  # :command.validations
  # :flag.validations
  if [[ -v args['--baseurl'] ]]; then
    validation_output="$(validate_base_url "${args['--baseurl']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--baseurl BASEURL" "$validation_output" >&2
      exit 1
    fi
  fi

  # :flag.validations
  if [[ -v args['--output'] ]]; then
    validation_output="$(validate_file_path "${args['--output']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--output, -o OUTPUT" "$validation_output" >&2
      exit 1
    fi
  fi

  # :command.whitelist_filter
  if [[ ${args['--format']:-} ]] && [[ ! ${args['--format']:-} =~ ^(json|yaml|xml|props)$ ]]; then
    printf "%s\n" "--format must be one of: json, yaml, xml, props" >&2
    exit 1
  fi

}

# :command.parse_requirements
keencli_show_ip_hotspot_summary_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      --help | -h)
        long_usage=yes
        keencli_show_ip_hotspot_summary_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="show ip hotspot summary"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in
      # :flag.case
      --metric_type)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--metric_type']="$2"
          shift
          shift
        else
          printf "%s\n" "--metric_type requires an argument: --metric_type METRIC_TYPE" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --count)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--count']="$2"
          shift
          shift
        else
          printf "%s\n" "--count requires an argument: --count COUNT" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --time_period)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--time_period']="$2"
          shift
          shift
        else
          printf "%s\n" "--time_period requires an argument: --time_period TIME_PERIOD" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --baseurl)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--baseurl']="$2"
          shift
          shift
        else
          printf "%s\n" "--baseurl requires an argument: --baseurl BASEURL" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --output | -o)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--output']="$2"
          shift
          shift
        else
          printf "%s\n" "--output requires an argument: --output, -o OUTPUT" >&2
          exit 1
        fi
        ;;

      # :flag.case
      --format | -f)

        # :flag.case_arg
        if [[ -n ${2+x} ]]; then
          args['--format']="$2"
          shift
          shift
        else
          printf "%s\n" "--format requires an argument: --format, -f FORMAT" >&2
          exit 1
        fi
        ;;

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        printf "invalid argument: %s\n" "$key" >&2
        exit 1

        ;;

    esac
  done

  # :command.default_assignments
  [[ -n ${args['--metric_type']:-} ]] || args['--metric_type']="sumbytes"
  [[ -n ${args['--count']:-} ]] || args['--count']="5"
  [[ -n ${args['--time_period']:-} ]] || args['--time_period']="3m"
  [[ -n ${args['--baseurl']:-} ]] || args['--baseurl']="http://192.168.1.1"
  [[ -n ${args['--format']:-} ]] || args['--format']="yaml"

  # :command.validations
  # :flag.validations
  if [[ -v args['--baseurl'] ]]; then
    validation_output="$(validate_base_url "${args['--baseurl']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--baseurl BASEURL" "$validation_output" >&2
      exit 1
    fi
  fi

  # :flag.validations
  if [[ -v args['--output'] ]]; then
    validation_output="$(validate_file_path "${args['--output']:-}")"
    if [[ -n "${validation_output}" ]]; then
      printf "validation error in %s:\n%s\n" "--output, -o OUTPUT" "$validation_output" >&2
      exit 1
    fi
  fi

  # :command.whitelist_filter
  if [[ ${args['--metric_type']:-} ]] && [[ ! ${args['--metric_type']:-} =~ ^(sumbytes|sumspeed)$ ]]; then
    printf "%s\n" "--metric_type must be one of: sumbytes, sumspeed" >&2
    exit 1
  fi
  if [[ ${args['--time_period']:-} ]] && [[ ! ${args['--time_period']:-} =~ ^(3m|1h|3h|24h)$ ]]; then
    printf "%s\n" "--time_period must be one of: 3m, 1h, 3h, 24h" >&2
    exit 1
  fi
  if [[ ${args['--format']:-} ]] && [[ ! ${args['--format']:-} =~ ^(json|yaml|xml|props)$ ]]; then
    printf "%s\n" "--format must be one of: json, yaml, xml, props" >&2
    exit 1
  fi

}

